<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.luxoft.studentinfo.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.luxoft.studentinfo.Perspective"
            name="RCP Perspective"
            id="com.luxoft.studentInfo.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.luxoft.studentinfo.GroupView"
            id="com.luxoft.studentInfo.GroupView"
            name="GroupView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.luxoft.studentinfo.InfoEditor"
            default="false"
            id="com.luxoft.studentInfo.InfoEditor"
            name="InfoEditor">
      </editor>
   </extension>

</plugin>
